{
    "name": "dysix/think-captcha",
    "description": "Modify the think-captcha composer package to apply to thinkphp+vue mode",
    "authors": [
        {
            "name": "dysix",
            "email": "245780976@qq.com"
        }
    ],
    "license": "Apache-2.0",
    "require": {
        "topthink/framework": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "think\\captcha\\": "src/"
        },
        "files": [
            "src/helper.php"
        ]
    },
    "extra": {
        "think": {
            "services": [
                "think\\captcha\\CaptchaService"
            ],
            "config": {
                "captcha": "src/config.php"
            }
        }
    }
}